{
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@js/*": ["./js/*"]
    }
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
